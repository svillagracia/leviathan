<md-toolbar layout="row">
  <button ng-click="toggleSidenav('left')" hide-gt-sm class="md-icon-button menuBtn">
  </button>
  <h1 class="md-toolbar-tools" layout-align-gt-sm="center">Leviathan</h1>
</md-toolbar>
<div layout="row" flex>
    <md-sidenav layout="column" class="md-sidenav-left md-whiteframe-z2" md-component-id="left" md-is-locked-open="$mdMedia('gt-sm')">
      <img class="logo" src="/images/Leviathan.png">
      <md-button ng-href="/" class="md-button md-default-theme">Home</md-button>
      <md-button href="/whiteboard" class="md-button md-default-theme">White Board</md-button>
      <md-button href="/weightroom" class="md-button md-default-theme">Weight Room</md-button>
      <md-button href="/logout" class="md-button md-default-theme">Log Out</md-button>
      <md-button href="/signup" class="md-button md-default-theme">Sign Up</md-button>
      <md-button href="/login" class="md-button md-default-theme">Log In</md-button>
      <md-button href="/about" class="md-button md-default-theme">About</md-button>
    </md-sidenav>
    <div layout="column" flex id="content">
        <md-content layout="column" flex class="md-padding">
          <ng-view></ng-view>
        </md-content>
    </div>
</div>
